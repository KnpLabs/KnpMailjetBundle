    "require": {
        "php": ">=5.3.8",
        "knplabs/mailjet-api-php": "*"
    },
    "autoload": {
        "psr-0": { "Knp\\Bundle\\MailjetBundle": "" }
    },
    "target-dir": "Knp/Bundle/MailjetBundle"
}